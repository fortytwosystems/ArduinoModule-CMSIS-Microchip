<?xml version="1.0" encoding="iso-8859-1"?>

<flash_device>
  <exe>$TOOLKIT_DIR$\config\flashloader\Atmel\sam3x8h\sam3x8h-flash.out</exe>
  <flash_base>0x00080000</flash_base>
  <page>1024</page>
  <block>32 0x4000</block>
  <macro>$TOOLKIT_DIR$\config\debugger\Atmel\Trace_SAM3XA.dmac</macro>
  <args_doc>[--flash] specifies the flash loader to program the internal flash.
[--boot] is given, device will boot from flash after programming (GPNVM bit set). 
</args_doc>
</flash_device>
